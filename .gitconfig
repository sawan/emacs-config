[alias]
    co = checkout
    st = status
    stat = status
    sta = stash
    dt = difftool
    mt = mergetool
    lo = log --oneline --decorate
    sdif = diff --ignore-all-space --name-status -C -M
    stash-p = stash pop
    stash-s = stash save
    maspull = ! git checkout master && git pull origin master
    pusho =   push origin HEAD
    pullo =   pull origin HEAD

[color]
    ui = auto

[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green

[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold

[color "status"]
    added = yellow
    changed = green bold
    untracked = cyan
